<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.CRFitness.Member.model.MemberVO" table="members" >
        <id name="member_Id" column="Member_Id">
            <generator class="generator.MemberGenerator"/>
        </id>
        <property name="e_mail" column="e_mail" />      
        <property name="password" column="password" />
        <property name="nickname" column="nickname" />
        <property name="registerdate" column="registerdate" />
        <property name="photo" type="binary">
            <column name="photo"  />
        </property>      

    </class>
</hibernate-mapping>