<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.CRFitness.OrderDetails.model.OrderDetailsVO" table="OrderDetails" >
        <id name="details_No" column="Details_No">
            <generator class="generator.OrderDetailsGenerator"/>
        </id>
        <property name="order_Id" column="Order_Id" />      
        <property name="product_Name" column="Product_Name" />
        <property name="quantity" column="Quantity" />
        <property name="size" column="Size" />
        <property name="color" column="Color" />
        <property name="amount" column="Amount" />
        
    </class>
</hibernate-mapping>