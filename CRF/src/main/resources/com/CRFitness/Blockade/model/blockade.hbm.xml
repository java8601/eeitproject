<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.CRFitness.Blockade.model.BlockadeVO" table="Blockade" >
        <composite-id>
            <!-- key-property標籤表示哪一些屬性對應複合主鍵 -->
            <key-property name="member_Id" column="Member_Id" />
            <key-property name="blockade_Id" column="Blockade_Id" />
        </composite-id>
        
        
    </class>
</hibernate-mapping>